<!DOCTYPE html>
<html lang="en">
<head>
    <!-- Meta Tags -->
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name="keywords" content="Site keywords here">
	<meta name="description" content="">
	<meta name='copyright' content=''>
	<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <title>@ViewData["Title"] - KHAS</title>

    <!-- Favicon -->
    <link rel="icon" href="img/favicon.ico">
    <!-- Google Fonts -->
    <link href="https://fonts.googleapis.com/css?family=Poppins:200i,300,300i,400,400i,500,500i,600,600i,700,700i,800,800i,900,900i&display=swap" rel="stylesheet">

    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="css/bootstrap.min.css" asp-append-version="true">
	<!-- Nice Select CSS -->
    <link rel="stylesheet" href="css/nice-select.css" asp-append-version="true">
	<!-- Font Awesome CSS -->
    <link rel="stylesheet" href="css/font-awesome.min.css" asp-append-version="true">
	<!-- icofont CSS -->
    <link rel="stylesheet" href="css/icofont.css" asp-append-version="true">
	<!-- Slicknav -->
    <link rel="stylesheet" href="css/slicknav.min.css" asp-append-version="true">
	<!-- Owl Carousel CSS -->
    <link rel="stylesheet" href="css/owl-carousel.css" asp-append-version="true">
	<!-- Datepicker CSS -->
    <link rel="stylesheet" href="css/datepicker.css" asp-append-version="true">
	<!-- Animate CSS -->
    <link rel="stylesheet" href="css/animate.min.css" asp-append-version="true">
	<!-- Magnific Popup CSS -->
    <link rel="stylesheet" href="css/magnific-popup.css" asp-append-version="true">
	
	<!-- Medipro CSS -->
    <link rel="stylesheet" href="css/normalize.css" asp-append-version="true">
    <link rel="stylesheet" href="css/style.css" asp-append-version="true">
    <link rel="stylesheet" href="css/responsive.css" asp-append-version="true">
</head>
<body>

    <header class="header">
        <!-- Header Inner -->
        <div class="header-inner">
            <div class="container">
                <div class="inner">
                    <div class="row">
                        <div class="col-lg-3 col-md-3 col-12">
                            <!-- Start Logo -->
                            <div class="logo">
                                <a href="index.html">
                                    <img src="img/logo.png" alt="#">
                                </a>
                            </div>
                            <!-- End Logo -->
                            <!-- Mobile Nav -->
                            <div class="mobile-nav"></div>
                            <!-- End Mobile Nav -->
                        </div>
                        <div class="col-lg-6 col-md-9 col-12">
                            <!-- Main Menu -->
                            <div class="main-menu">
                                <nav class="navigation">
                                    <ul class="nav menu">
                                        <li class="active">
                                            <a asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                                        </li>
                                        <li><a href="contact.html">Contact Us</a></li>
                                    </ul>
                                </nav>
                            </div>
                            <!--/ End Main Menu -->
                        </div>
                        <div class="col-lg-3 col-12">
                            <div class="get-quote">
                                <a asp-area="" asp-controller="" asp-action="" class="btn">Patient</a>

                                <a asp-area="" asp-controller="" asp-action="" class="btn">Doctor</a>
                            </div>


                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!--/ End Header Inner -->
    </header>
    <!-- End Header Area -->

     <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <!-- Footer Area -->
    <footer id="footer" class="footer ">
        <!-- Footer Top -->
        <div class="footer-top">
            <div class="container">
                <div class="row">
                    <div class="col-lg-3 col-md-6 col-12">
                        <div class="single-footer">
                            <h2>About Us</h2>
                            <p>Khas</p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!--/ End Footer Top -->
        <!-- Copyright -->
        <div class="copyright">
            <div class="container">
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-12">
                        <div class="copyright-content">
                            <p> KHAS </p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!--/ End Copyright -->
    </footer>
    <!--/ End Footer Area -->

   
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    <!-- Slider -->
    <script src="js/jquery-migrate-3.0.0.js" asp-append-version="true"></script>
    <script src="js/jquery-ui.min.js" asp-append-version="true"></script>>
    <script src="js/slicknav.min.js" asp-append-version="true"></script>
    <script src="js/owl-carousel.js" asp-append-version="true"></script>
    <script src="js/bootstrap.min.js" asp-append-version="true"></script>
    <script src="js/main.js" asp-append-version="true"></script>
    <!-- Slider -->

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>

